        <div class="container-fluid">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <% @breadcrumb.each do |b| %>
                        <li class='breadcrumb-item'>
                            <a href='<%= b[:link] %>'>
                                <%= truncate(b[:name],length: 70) %>
                            </a>
                        </li>
                    <% end %>
                    &nbsp&nbsp&nbsp&nbsp<%= notice %>  <%= alert %>
                </ol>
            </nav>
        </div>